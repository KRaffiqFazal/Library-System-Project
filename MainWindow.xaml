<Window x:Class="Library_System.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Library_System"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Background="#FF94E9F9">
    <Grid Background="#FFD7F9F1" Margin="0,0,-1120,-646">
        <Image x:Name="picLogo" HorizontalAlignment="Left" Height="328" VerticalAlignment="Top" Width="414" Source="/logo.png" Stretch="UniformToFill" Margin="0,10,0,0"/>
        <Button x:Name="btnProceed" Content="Button" HorizontalAlignment="Center" VerticalAlignment="Center" Width="960" Height="540"/>

    </Grid>
</Window>
